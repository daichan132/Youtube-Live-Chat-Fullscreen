{
  "name": "youtube-live-chat-fullscreen",
  "private": true,
  "version": "2.2.7",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "submit:chrome:v2": "node scripts/cws-v2-submit.mjs",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "format": "biome format --write ./entrypoints/** ./shared/**",
    "lint": "biome check --write ./entrypoints/** ./shared/** && tsc --noEmit",
    "test:unit": "vitest run",
    "prepare": "lefthook install",
    "e2e": "playwright test"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "classnames": "^2.5.1",
    "i18next": "^25.8.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "immer": "^11.1.3",
    "lodash-es": "^4.17.23",
    "re-resizable": "^6.11.2",
    "react": "19.2.4",
    "react-color": "^2.19.3",
    "react-dom": "19.2.4",
    "react-i18next": "^16.5.4",
    "react-icons": "^5.5.0",
    "react-modal": "^3.16.3",
    "react-transition-group": "^4.4.5",
    "react-use": "^17.6.0",
    "redux-persist-webextension-storage": "^1.0.2",
    "uuid": "^13.0.0",
    "vite-tsconfig-paths": "^6.1.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@playwright/test": "1.58.2",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.2",
    "@types/chrome": "0.1.36",
    "@types/lodash-es": "4.17.12",
    "@types/react": "19.2.13",
    "@types/react-color": "3.0.13",
    "@types/react-dom": "19.2.3",
    "@types/react-modal": "3.16.3",
    "@types/react-router-dom": "5.3.3",
    "@types/react-transition-group": "4.4.12",
    "@types/redux-persist-webextension-storage": "1.0.3",
    "@types/uuid": "11.0.0",
    "@wxt-dev/module-react": "1.1.5",
    "@wxt-dev/unocss": "1.0.1",
    "jsdom": "28.0.0",
    "lefthook": "2.1.0",
    "typescript": "5.9.3",
    "unocss": "66.6.0",
    "vitest": "4.0.18",
    "wxt": "0.20.14"
  },
  "resolutions": {
    "@isaacs/brace-expansion": "5.0.1",
    "brace-expansion": "2.0.2",
    "glob": "10.5.0",
    "lodash": "4.17.23",
    "lodash-es": "4.17.23",
    "node-forge": "1.3.2",
    "tar": "7.5.4",
    "vite": "7.3.1"
  },
  "packageManager": "yarn@4.12.0",
  "browserslist": [
    "last 2 version",
    "ie >= 11",
    "iOS >= 8.1",
    "Android >= 4.4"
