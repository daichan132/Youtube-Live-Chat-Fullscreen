{
  "name": "browser-extension-react-ts-starter",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "dev": "run-s clean && run-p dev:scss vite",
    "vite": "vite",
    "dev:scss": "typed-scss-modules src  --watch",
    "build": "run-s clean && tsc && vite build && run-s firefox-mv2-build",
    "firefox-mv2-build": "node tools/unstable_mv3ToMv2Build.js",
    "clean": "run-p clean:dist && run-p clean:dist-firefox-v2",
    "clean:dist": "rimraf dist",
    "clean:dist-firefox-v2": "rimraf dist-firefox-v2",
    "format": "run-p format:eslint && run-p format:prettier",
    "format:eslint": "run-s \" lint:eslint --fix \"",
    "format:prettier": "run-s \" lint:prettier --write \"",
    "lint": "run-p lint:eslint && run-p lint:prettier && run-p lint:type",
    "lint:eslint": "eslint . --ext .js,.ts,.tsx --max-warnings 0 --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.(md|json|yml)\" --ignore-path .gitignore --check",
    "lint:type": "tsc --noEmit",
    "test": "jest",
    "ci:test": "run-s \" test --ci --reporters=\"default\" --reporters=\"github-actions\" \"",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "classnames": "^2.3.2",
    "emoji-regex": "^10.3.0",
    "lodash-es": "^4.17.21",
    "re-resizable": "^6.9.11",
    "react": "18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "18.2.0",
    "react-icons": "^4.12.0",
    "react-modal": "^3.16.1",
    "react-transition-group": "^4.4.5",
    "react-use": "^17.4.0",
    "redux-persist-webextension-storage": "^1.0.2",
    "webext-zustand": "^0.2.0",
    "zustand": "^4.4.6"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.21",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.10",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.38",
    "@types/react-color": "^3.0.10",
    "@types/react-dom": "^18.2.17",
    "@types/react-modal": "^3.16.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.9",
    "@types/redux-persist-webextension-storage": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "css-declaration-sorter": "^7.1.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "^11.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-chrome": "^0.8.0",
    "jest-environment-jsdom": "^29.7.0",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.5",
    "simple-git-hooks": "^2.9.0",
    "ts-jest": "^29.1.1",
    "typed-scss-modules": "^7.1.4",
    "typescript": "5.3.2",
    "vite": "^4.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged"
  },
  "packageManager": "yarn@4.0.1",
  "browserslist": [
    "last 2 version",
    "ie >= 11",
    "iOS >= 8.1",
    "Android >= 4.4"
  ]
}
